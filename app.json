{
  "expo": {
    "name": "expresscash",
    "slug": "clase-19",
    "newArchEnabled": true,
    "version": "1.0.0",
    "scheme": "expresscash",
    "orientation": "portrait",
    "fonts": [
      {
        "asset": "@expo-google-fonts/poppins"
      }
    ],
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "center",
      "backgroundColor": "#004D56"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]

  }
}
